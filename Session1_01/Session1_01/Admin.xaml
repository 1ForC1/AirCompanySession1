<Window x:Class="Session1_01.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Session1_01"
        mc:Ignorable="d"
        Title="Admin" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <DataGrid Height="300" Width="650" x:Name="dataGrid" CanUserAddRows="False" CanUserDeleteRows="False" IsReadOnly="True"/>
            <Button Width="100" Height="40" x:Name="ExitBtn" Content="ExitBtn" Click="ExitBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
